  <body>
    <header>
      <div class="user-info">
        <div id="logo">
          <h1>Slelp</h1>
        </div>
        <div id="add-search-container" class="DesktopSearchContainer">
          <button class="add-help">
            <h1>Add Help +</h1>
          </button>
          <button class="search">
            <img class="search-img" src="/images/search-128.png" alt="">
          </button>
        </div>
        <div class="group">
          <h1>{{data}}</h1>
        </div>

      </div>

